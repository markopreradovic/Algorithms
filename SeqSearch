int SeqSearch (<tip> niz[], <tip> kljuc, int n) 
{
   for (int i=0; i<n; i++) 
      if (niz[i] == kljuc) return i;
   return -1;
}

int SeqSearchStraza (<tip> niz[], <tip> kljuc) 
{
   int i=0;
   while (niz[i] != kljuc) i++; 
   return i;
}

int SeqSorted(<tip> niz[], <tip> kljuc, int n) 
{
   int i=0;
   while (niz[i]<kljuc && i<n) i++; 
   return (niz[i]==kljuc && i<n);
}

